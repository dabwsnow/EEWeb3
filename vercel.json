{
  "functions": {
    "app/main.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "10mb" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ]
}
